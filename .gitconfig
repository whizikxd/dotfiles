[include]
  path = ~/.gitconfig-personal

[init]
  defaultBranch = master

[alias]
  a = add

  b = branch

  c = commit
  cp = cherry-pick

  ch = checkout

  s = status

  l = log
  gl = log --graph
  sl = shortlog
  rl = reflog

  d = diff

  sh = show

[merge]
  log = true # 20
  ff = false

[pull]
  ff = true
